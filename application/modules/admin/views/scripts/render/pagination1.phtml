<?php if (($this->pageCount)>1): ?>	<div class="paginationControl">		<?php if (isset($this->previous)): ?>			<a href="<?= $this->url(array('pagina' => $this->first)); ?>" class="first">Primeira</a>		<?php else: ?>			<span class="first-disabled">Primeira</span>		<?php endif; ?>		<?php if (isset($this->previous)): ?>			<a href="<?= $this->url(array('pagina' => $this->previous)); ?>" class="prev"></a>		<?php else: ?>			<span class="prev-disabled"></span>		<?php endif; ?>		<?php foreach ($this->pagesInRange as $page): ?>			<?php if ($page != $this->current): ?>				<a href="<?= $this->url(array('pagina' => $page)); ?>" class="numero"><span><?= $page; ?></span></a>			<?php else: ?>				<span class="current"><span><?= $page; ?></span></span>			<?php endif; ?>		<?php endforeach; ?>		<?php if (isset($this->next)): ?>			<a href="<?= $this->url(array('pagina' => $this->next)); ?>" class="next"></a>		<?php else: ?>			<span class="next-disabled"></span>		<?php endif; ?>		<?php if (isset($this->next)): ?>			<a href="<?= $this->url(array('pagina' => $this->last)); ?>" class="last">Última</a>		<?php else: ?>			<span class="last-disabled">Última</span>		<?php endif; ?>	</div><?php endif;?>
